.canvas {
    width: 750rpx;
    height: 750rpx;
}
.canvas.pro {
    position: absolute;
    bottom: 0;
    left: 0;
    transform: translate3d(-999rpx, 0, 0);
}
.canvas.debug {
    position: absolute;
    top: 0;
    left: 0;
    border: 1rpx solid #ccc;
    z-index: 99999;
}
