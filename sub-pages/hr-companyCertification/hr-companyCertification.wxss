page {
  background: #fff; }

.table {
  border-top: 1px solid #d9d9d9;
  border-left: 1px solid #d9d9d9; }
  .table .tr {
    display: flex;
    font-size: 25rpx;
    color: #666666; }
  .table .td {
    border-right: 1px solid #d9d9d9;
    border-bottom: 1px solid #d9d9d9;
    min-height: 64rpx;
    display: flex;
    align-items: center;
    padding: 5px; }
    .table .td:first-child {
      background: #f5f5f5;
      flex: 0 0 40%; }
    .table .td:last-child {
      flex: 1;
      color: #999999; }
    .table .td text {
      color: #2B27DD; }
